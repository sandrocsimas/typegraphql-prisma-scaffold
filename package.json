{
  "name": "typegraphql-prisma-scaffold",
  "version": "1.0.0",
  "description": "TypeGrapQL + Prisma Scaffold",
  "license": "N/A",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run lint && npx rimraf lib && tsc --build src && cp .env lib",
    "deps-start": "sh bin/deps-start.sh",
    "deps-stop": "sh bin/deps-stop.sh",
    "lint": "eslint .",
    "prisma-generate": "npx prisma generate",
    "prisma-format": "npx prisma format",
    "prisma-migrate": "npx prisma migrate dev --name",
    "prisma-push": "npx prisma db push",
    "start": "npm run prisma-generate && npm run build && node lib/server.js",
    "test": "mocha --recursive --require ts-node/register --extensions ts **/*.spec.ts"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/chai": "4.3.4",
    "@types/chai-as-promised": "7.1.5",
    "@types/chai-string": "1.4.2",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.14",
    "@types/mocha": "10.0.1",
    "@types/node": "18.11.12",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "chai": "4.3.7",
    "eslint": "8.29.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "mocha": "10.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@apollo/server": "4.2.2",
    "@prisma/client": "4.7.1",
    "body-parser": "1.20.1",
    "class-validator": "0.13.2",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-jwt": "7.7.7",
    "graphql": "16.6.0",
    "jsonwebtoken": "8.5.1",
    "mysql2": "2.3.3",
    "prisma": "4.7.1",
    "reflect-metadata": "0.1.13",
    "type-graphql": "2.0.0-beta.1",
    "typedi": "0.10.0"
  },
  "bundleDependencies": [
    "@generated/type-graphql"
  ]
}
